# Database Configuration
POSTGRES_DB=fuar_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# Spring Boot Configuration
SPRING_PROFILES_ACTIVE=docker
SERVER_PORT=8080

# JWT Configuration
JWT_SECRET_KEY=your_jwt_secret_key_here_minimum_256_bits
